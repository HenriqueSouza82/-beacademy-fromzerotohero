Algoritmo "CADASTRO - NOME - ENDERECO"
var nome , endereco : caractere // variaveis nome e endereço
inicio
      escreva ("Digite seu Nome: ")
      leia (nome)
      escreva ("Digite seu Endereço: ")
      leia (endereco)
      escreva ("Nome: " + nome)
      escreva ("Endereço: " + endereco)
Fimalgoritmo